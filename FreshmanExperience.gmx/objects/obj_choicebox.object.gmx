            <kind>1</kind>
            <string>///Draw
var pad = 20;
draw_set_font(fnt_text);
//find biggest width
var height = string_height(header)+pad*6;
var bigW = string_width(header) + pad;
for (i=0; i&lt;ds_list_size(options); i+=1)
{
    var str =options[|i];
    height += string_height(str);
    height += pad*0.5;
    if (string_width(str) &gt; bigW) {
        bigW = string_width(str) + pad;
    }
};

var xm = room_width*0.5;
var ym = room_height*0.4;
var y1 = ym-height*0.5-pad;
var y2 = ym+height*0.5+pad;
var x1 = xm-bigW*0.5-pad;
var x2 = xm+bigW*0.5+pad;
draw_set_color(c_black);
draw_rectangle(x1, y1, x2, y2, false);
draw_set_valign(fa_top);
draw_set_halign(fa_middle);

draw_set_color(c_white);
draw_text(xm, y1+pad, header);

var yy = y1+pad+string_height(header)+pad;
for (i=0; i&lt;ds_list_size(options); i+=1)
{
    var str = options[|i];
    var by1 = yy;
    var by2 = yy+pad+string_height(str);
    var bx1 = x1 + pad;
    var bx2 = x2 - pad;
    draw_set_color(c_dkgray);
    var mx, my;
    mx = mouse_x; my = mouse_y;
    if(os_browser != browser_not_a_browser) {
        mx = window_mouse_get_x() -window_get_x();
        my = window_mouse_get_y() -window_get_y();
    }
    if (point_in_rectangle(mx, my, bx1, by1, bx2, by2)) {
        draw_set_color(c_gray);
        if (mouse_check_button_released(mb_left)) {
            //Option Selected
            chosenIndex = i;
            event_perform(ev_other, ev_user0);
        }
    }
    draw_rectangle(bx1, by1, bx2, by2, false);
    draw_set_color(c_white);
    draw_set_valign(fa_center);
    draw_text(xm,yy+(by2-by1)*0.5, str);
    yy += by2 - by1 +pad*0.5;
};





</string>
          </argument>
